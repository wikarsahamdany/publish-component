.card {
  min-height: 12vh;
  margin: 0vh 4vw;
  padding: 1.5vh 0vw;
  box-shadow: 0px 2px 6px rgba(0, 0, 0, 0.3);
  border-radius: 5px;
}

.content{
  display: flex;
  flex-direction: column;
  gap: 10px;
}

.test {
  width: 16vw;
  height: 8vh;
  border-radius: 50px;
  background: gray;
}

.view {
  display: flex; 
  justify-content: space-around;
}
